{
  "name": "warikankun-server",
  "version": "1.0.0",
  "main": "build/src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "functions-framework --source=build/src/ --target=handler --port=4000",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec pnpm start\"",
    "deploy": "gcloud functions deploy warikankun-server --region=asia-northeast1 --entry-point handler --runtime nodejs16 --trigger-http --memory 128MB",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "pnpm run compile",
    "posttest": "pnpm run lint"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@prisma/client": "4.3.1",
    "apollo-server": "^3.10.2",
    "apollo-server-cloud-functions": "^3.10.2",
    "class-validator": "^0.13.2",
    "graphql": "^15.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "prisma": "4.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~4.7.0",
    "gts": "^3.1.1"
  }
}
